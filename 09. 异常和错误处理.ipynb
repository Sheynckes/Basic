   "source": [
    "# 异常的抛出和捕捉"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "No Exception\n",
      "Always done\n",
      "10.0\n"
     ]
    }
   ],
   "source": [
    "import traceback\n",
    "\n",
    "try:\n",
    "    r = 10 / 1\n",
    "# 有异常发生时，会执行except中的代码段\n",
    "except ZeroDivisionError as e:\n",
    "    print(e)\n",
    "    r = 1\n",
    "# 没有异常发生时，会执行else中的代码段\n",
    "else:\n",
    "    print('No Exception')\n",
    "# 不管异常是否发生，都会执行finally中的代码段\n",
    "finally:\n",
    "    print('Always done')\n",
    "print(r)"
   ]
  },
